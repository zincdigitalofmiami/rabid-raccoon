// ─── Options Data ─────────────────────────────────────────────────────────

/// Daily options statistics metrics per parent symbol.
/// Raw per-contract data lives in parquet files (datasets/options-statistics/, options-ohlcv/).
/// This table stores aggregated daily summaries for the dashboard and ML feature pipeline.
model MktOptionsStatistics1d {
  id              BigInt     @id @default(autoincrement())
  parentSymbol    String     @db.VarChar(16) // ES.OPT, NQ.OPT, etc.
  eventDate       DateTime   @db.Date
  totalVolume     BigInt?                    // sum of cleared volume across all contracts
  totalOI         BigInt?                    // sum of open interest across all contracts
  settlement      Decimal?   @db.Decimal(18, 6) // front-month settlement price
  avgIV           Decimal?   @db.Decimal(10, 6) // volume-weighted avg implied volatility
  contractCount   Int?                       // number of active contracts that day
  source          DataSource @default(DATABENTO)
  sourceDataset   String?    @db.VarChar(64)
  sourceSchema    String?    @db.VarChar(32)
  ingestedAt      DateTime   @default(now()) @db.Timestamptz(6)
  knowledgeTime   DateTime   @default(now()) @db.Timestamptz(6)
  rowHash         String?    @db.VarChar(64)
  metadata        Json?                      // put/call breakdown, skew, GEX, etc.

  @@unique([parentSymbol, eventDate], map: "mkt_options_statistics_1d_parent_date_key")
  @@index([eventDate], map: "mkt_options_statistics_1d_date_idx")
  @@index([parentSymbol], map: "mkt_options_statistics_1d_parent_idx")
  @@map("mkt_options_statistics_1d")
}

/// Daily OHLCV for options rolled to parent symbol level.
/// Aggregated from per-contract ohlcv-1d parquet data.
model MktOptionsOhlcv1d {
  id              BigInt     @id @default(autoincrement())
  parentSymbol    String     @db.VarChar(16) // ES.OPT, NQ.OPT, etc.
  eventDate       DateTime   @db.Date
  totalVolume     BigInt?                    // sum of volume across all contracts
  contractCount   Int?                       // number of contracts traded
  avgClose        Decimal?   @db.Decimal(18, 6) // volume-weighted avg close (premium)
  maxHigh         Decimal?   @db.Decimal(18, 6) // highest high across all contracts
  minLow          Decimal?   @db.Decimal(18, 6) // lowest low across all contracts
  source          DataSource @default(DATABENTO)
  sourceDataset   String?    @db.VarChar(64)
  sourceSchema    String?    @db.VarChar(32)
  ingestedAt      DateTime   @default(now()) @db.Timestamptz(6)
  knowledgeTime   DateTime   @default(now()) @db.Timestamptz(6)
  rowHash         String?    @db.VarChar(64)
  metadata        Json?                      // front-month vs back-month breakdown, etc.

  @@unique([parentSymbol, eventDate], map: "mkt_options_ohlcv_1d_parent_date_key")
  @@index([eventDate], map: "mkt_options_ohlcv_1d_date_idx")
  @@index([parentSymbol], map: "mkt_options_ohlcv_1d_parent_idx")
  @@map("mkt_options_ohlcv_1d")
}
