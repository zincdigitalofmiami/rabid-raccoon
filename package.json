{
  "name": "rabid-raccoon",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "npm run lint:symbol-hardcodes && eslint .",
    "lint:symbol-hardcodes": "node scripts/lint-symbol-hardcodes.mjs",
    "lint:symbol-hardcodes:baseline": "node scripts/lint-symbol-hardcodes.mjs --write-baseline",
    "mcp:doctor": "node scripts/mcp-doctor.mjs",
    "mcp:check": "node scripts/check-mcp-drift.mjs",
    "backtest": "tsx scripts/backtest-signals.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "db:migrate": "prisma migrate deploy",
    "db:counts": "tsx scripts/db-counts.ts",
    "db:sync:local": "tsx scripts/db-sync-local.ts",
    "ingest:market": "tsx scripts/ingest-market-prices.ts",
    "ingest:market:daily": "tsx scripts/ingest-market-prices-daily.ts",
    "ingest:macro": "tsx scripts/ingest-macro-indicators.ts",
    "ingest:alt-news": "tsx scripts/ingest-alt-news-feeds.ts",
    "ingest:fred-news": "tsx scripts/ingest-fred-news.ts",
    "ingest:mm": "tsx scripts/ingest-mm-signals.ts",
    "ingest:mes:live": "tsx scripts/ingest-mes-live-15m.ts",
    "ingest:mes:live:stream": "tsx scripts/ingest-mes-live-stream.ts",
    "ingest:mes:live:databento": ".venv-finance/bin/python scripts/ingest-mes-live-databento.py",
    "ingest:mes:backfill": "tsx scripts/backfill-mes-15m-1h-1d.ts",
    "ingest:all": "tsx scripts/ingest-all.ts",
    "registry:snapshot": "tsx scripts/generate-symbol-registry-snapshot.ts",
    "ingest:fred:complete": "tsx scripts/ingest-fred-complete.ts",
    "econ:series:reconcile": "tsx scripts/reconcile-economic-series.ts",
    "import:fusion:econ": "tsx scripts/import-fusion-econ.ts",
    "dataset:autogluon:1h": "tsx scripts/build-complete-dataset.ts",
    "dataset:autogluon:15m": "tsx scripts/build-15m-dataset.ts",
    "dataset:lean:1h": "tsx scripts/build-lean-dataset.ts --timeframe=1h --out=datasets/autogluon/mes_lean_fred_indexes_2020plus.csv",
    "train:core:preflight": ".venv-autogluon/bin/python scripts/train-final.py --preflight-only --dataset datasets/autogluon/mes_lean_fred_indexes_2020plus.csv",
    "train:core:prod": ".venv-autogluon/bin/python scripts/train-final.py --clean --dataset datasets/autogluon/mes_lean_fred_indexes_2020plus.csv",
    "predict:core:refresh": ".venv-autogluon/bin/python scripts/predict.py --rows=24 --dataset datasets/autogluon/mes_lean_fred_indexes_2020plus.csv --output public/ml-predictions.json",
    "train:core:ready": "npm run dataset:lean:1h && npm run train:core:preflight",
    "test:feature-availability": "node --import tsx --test scripts/feature-availability.test.ts",
    "test:db-routing": "node --import tsx --test scripts/db-url-contract.test.ts",
    "inngest:dev": "npx inngest-cli dev -u http://localhost:3000/api/inngest"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.78.0",
    "@prisma/adapter-pg": "^7.4.1",
    "@prisma/client": "^7.4.1",
    "@prisma/extension-accelerate": "^3.0.1",
    "inngest": "^3.52.3",
    "lightweight-charts": "^5.1.0",
    "next": "^16.1.6",
    "openai": "^6.22.0",
    "pg": "^8.18.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "overrides": {
    "hono": ">=4.12.2",
    "lodash": ">=4.17.23",
    "glob": ">=10.5.0",
    "minimatch": ">=10.2.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.2.1",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.39.3",
    "eslint-config-next": "^16.1.6",
    "postcss": "^8.5.6",
    "prisma": "^7.4.1",
    "tailwindcss": "^4.2.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.3"
  }
}
